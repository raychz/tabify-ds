   "source": [
    "class processing_text:\n",
    "    def __init__(self, doc):\n",
    "        self.doc = str(doc).lower()\n",
    "        \n",
    "    def _remove_punctuation(self):\n",
    "        no_punc = \"\".join([character for character in self.doc if character not in string.punctuation])\n",
    "        return no_punc\n",
    "   \n",
    "    def _tokenize(self):\n",
    "        no_punc = self._remove_punctuation()\n",
    "        tokens = re.split('\\W+',no_punc)\n",
    "        return tokens\n",
    "    \n",
    "    def _remove_stopwords(self):\n",
    "        tokens = self._tokenize()\n",
    "        stopwords = nltk.corpus.stopwords.words('english')\n",
    "        no_stopwords = [word for word in tokens if word not in stopwords]\n",
    "        return no_stopwords\n",
    "    \n",
    "    def _stemming(self):\n",
    "        remove_stopwords = self._remove_stopwords()\n",
    "        porter_stemmer = nltk.PorterStemmer()\n",
    "        stems = [porter_stemmer.stem(word) for word in remove_stopwords]\n",
    "        return stems\n",
    "    \n",
    "    def fixed_text(self):\n",
    "        stemming = self._stemming()\n",
    "        fixed_text = \" \".join([word for word in stemming])\n",
    "        return fixed_text"
   ]
  },
